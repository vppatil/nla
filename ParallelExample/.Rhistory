library(inline)
doublematrix <- cxxfunction(signature(x = "numeric"), body = '
Rcpp::NumericMatrix xcpp(x);
int nrows = xcpp.nrow();
int ncolumns = xcpp.ncol();
for (int i = 0; i < nrows; i++){
for (int j = 0; j < ncolumns; j++){
xcpp[nrows * j + i] *= 2;
}
}
return xcpp;
', plugin="Rcpp")
